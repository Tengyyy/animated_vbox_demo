<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="stackPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.queue_demo.MenuController">
   <children>
      <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0">
         <StackPane.margin>
            <Insets right="50.0" />
         </StackPane.margin>
         <content>
            <VBox fx:id="scrollContent">
               <children>
                  <StackPane fx:id="historyWrapper" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="0.0" minWidth="-Infinity" prefWidth="200.0" />
               </children></VBox>
         </content>
      </ScrollPane>
      <Button fx:id="playPreviousButton" mnemonicParsing="false" onAction="#playPrevious" text="&lt;---" translateX="-60.0" StackPane.alignment="TOP_RIGHT">
         <StackPane.margin>
            <Insets top="10.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="playNextButton" mnemonicParsing="false" onAction="#playNext" text="---&gt;" StackPane.alignment="TOP_RIGHT">
         <StackPane.margin>
            <Insets top="10.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="historyButton" mnemonicParsing="false" onAction="#historyClick" text="Open/close history" translateY="50.0" StackPane.alignment="TOP_RIGHT" />
      <Button fx:id="shuffleButton" mnemonicParsing="false" onAction="#shuffle" text="Shuffle" translateY="-60.0" StackPane.alignment="CENTER_RIGHT" />
      <Button fx:id="switcherAddButton" mnemonicParsing="false" onAction="#addToSwitcher" text="Add to switcher" translateY="-30.0" StackPane.alignment="CENTER_RIGHT" />
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#add" text="Add to bottom" StackPane.alignment="CENTER_RIGHT" />
      <Button fx:id="addRandButton" mnemonicParsing="false" onAction="#addRand" text="Add to random" translateY="30.0" StackPane.alignment="CENTER_RIGHT" />
      <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clear" text="Clear" translateY="60.0" StackPane.alignment="CENTER_RIGHT" />
      <CheckBox fx:id="playToggle" mnemonicParsing="false" text="Play toggle" translateY="85.0" StackPane.alignment="CENTER_RIGHT" />
      <CheckBox fx:id="shufflePlayToggle" mnemonicParsing="false" text="Shuffle play" translateY="110.0" StackPane.alignment="CENTER_RIGHT" />
   </children>
</StackPane>
